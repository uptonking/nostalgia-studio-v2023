{
  "name": "@examples-hub/app-logux",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo-task-list": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.task1.cjs",
    "demo-task-sync-client": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.task-sync.cjs",
    "demo-task-sync-server": "node --loader=ts-node/esm/transpile-only --experimental-specifier-resolution=node  ./src-task-list-sync/server/index.ts ",
    "demo-task-sync-server-build": "webpack --stats-error-details --config webpack/webpack.config.task-sync-server.cjs",
    "demo-task-sync-server1": "node --loader=ts-node/esm/transpile-only --experimental-specifier-resolution=node  ./src-task-list-sync/server/index.ts ",
    "demo-task-sync-server2": "node --loader=tsm --experimental-specifier-resolution=node  ./src-task-list-sync/server/index.ts ",
    "start": "npm run demo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:log": "node index.js --name nested-a"
  },
