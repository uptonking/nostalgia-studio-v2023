{
  "name": "@examples-hub/crdt-tories",
  "version": "1.1.1111",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo:idbsidesync": "webpack serve --hot --config webpack/webpack.config.idbsync.cjs",
    "demo:idbsidesync-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./idb-side-sync/server/index.ts",
    "demo:woot-diff": "webpack serve --hot --config webpack/webpack.config.wootdiff.cjs",
    "demo:woot-diff-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./woot-diff/server.ts",
    "demo:logoot-list": "webpack serve --hot --config webpack/webpack.config.logootlist.cjs",
    "demo:logoot-list-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./logoot-list/server.ts",
    "demo:rga-ace": "webpack serve --hot --config webpack/webpack.config.rgaace.cjs",
    "demo:rga-ace-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./rga-automerge/ace-server.ts",
    "demo:json-fixed": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./json-fixed/simple-fixed-json-crdt.ts",
    "demo:crdt-hlc": "serve ./crdt-example-app-hlc-jlongster",
    "demo:crdt-hlc-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./crdt-example-app-hlc-jlongster/server/index.js",
    "demo:gdrive": "cd public && serve -C -l 8999",
    "start": "npm run demo:json-fixed",
    "server": "npm run demo:rga-ace-server",
    "test": "npm run test:shelf",
    "test:rga-peer": "jest ./rga-automerge/rga-peer.test.ts",
    "test:rga-crunch": "mocha --experimental-loader=ts-node/esm/transpile-only --experimental-specifier-resolution=node --extensions ts,js 'rga-automerge/crunch-rga.test.ts' --reporter spec --timeout 10000",
    "test:map-lww": "jest ./crdt-map/lww-map.test.ts",
    "test:shelf": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./json-fixed/shelf.test.ts",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "ace-builds": "^1.15.3",
    "better-sqlite3": "^8.3.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "diff-match-patch": "^1.0.5",
    "express": "^4.18.2",
    "murmurhash": "^0.0.2",
    "polka": "^0.5.2",
    "serve": "^14.0.1",
    "sirv": "^2.0.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "uuid": "^9.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/ace": "^0.0.48",
    "@types/better-sqlite3": "^7.6.4",
    "@types/ws": "^8.5.3",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2"
  }
}
