    "@hocuspocus/extension-sqlite": "^1.0.0-alpha.16",
    "@hocuspocus/provider": "^1.0.0-alpha.36",
    "@hocuspocus/server": "^1.0.0-alpha.102",
    "@slate-yjs/core": "0.3.1111",
    "@slate-yjs/react": "0.3.1111",
