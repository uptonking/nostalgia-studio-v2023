{
  "name": "@datalking/noseditor",
  "version": "0.0.1",
  "description": "A collabortive block-style editor built with slate.",
  "license": "(MIT OR AGPL-3.0-or-later)",
  "author": "jinyaoo <jinyaoo@qq.com>",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "//t": "temporary task shortcut",
    "t": "npm run build:design-tokens && npm run build:design-tokens",
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "build:design-tokens": "npx co build && node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./scripts/styles/convert-tokens.ts",
    "build:demo": "cross-env APP_ENV=react NODE_ENV=production webpack --config webpack/webpack.config.demo.prod.cjs",
    "demo": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo-yjs-client": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.yjs.cjs",
    "demo-yjs-server": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./example-server/index.ts",
    "start": "npm run demo",
    "server": "npm run demo-yjs-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "3.0.1111",
    "@dnd-kit/core": "6.0.8111",
    "@dnd-kit/modifiers": "6.0.1111",
    "@dnd-kit/sortable": "7.0.2111",
    "@dnd-kit/utilities": "3.2.1111",
    "@floating-ui/core": "1.2.5111",
    "@floating-ui/dom": "1.2.5111",
    "@floating-ui/react": "0.22.2111",
    "@floating-ui/react-dom": "2.0.1111",
    "@icon-park/react": "^1.4.2",
    "@linaria/core": "^4.5.4",
    "@slate-yjs/core": "0.3.1111",
    "@slate-yjs/react": "0.3.1111",
    "clsx": "^2.1.0",
    "event-emitter": "^0.3.5",
    "image-extensions": "^1.1.0",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "nanoid": "^5.0.4",
    "randomcolor": "^0.6.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-resize-detector": "^7.0.0",
    "slate": "0.78.1111",
    "slate-history": "0.66.1111",
    "slate-hyperscript": "0.77.1111",
    "slate-react": "0.79.1111",
    "tree-crawl": "^1.1.0",
    "yjs": "13.5.4111",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@cobalt-ui/cli": "^1.3.0",
    "@cobalt-ui/plugin-css": "^1.3.0",
    "@cobalt-ui/plugin-js": "^1.2.3",
    "@cobalt-ui/plugin-sass": "^1.2.3",
    "@faker-js/faker": "^8.3.1",
    "@linaria/babel-preset": "^4.5.4",
    "@linaria/webpack5-loader": "^4.5.4",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
