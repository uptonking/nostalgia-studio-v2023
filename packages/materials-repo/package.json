{
  "name": "@datalking/materials-repo",
  "version": "1.0.0",
  "description": "knowledge base powered by markdown/mdx files.",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:demo": "rimraf dist && cross-env APP_ENV=react webpack --config webpack/webpack.config.demo.js",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "cross-env APP_ENV=reactfresh webpack serve --hot --config webpack/webpack.config.demo.js",
    "dev:server": "cd server-node && ts-node-dev --respawn --transpile-only server.ts",
    "start": "npm run demo",
    "start2": "npm run dev:server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "@observablehq/plot": "^0.2.0",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "classnames": "^2.3.2",
    "codemirror": "^5.62.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "d3": "^7.0.1",
    "express-session": "^1.17.2",
    "@faker-js/faker": "^8.0.2",
    "fork-awesome": "^1.2.0",
    "jquery": "^3.6.0",
    "keyboardjs": "^2.6.4",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.32",
    "multer": "^1.4.3",
    "pretty-bytes": "^5.6.0",
    "prosemirror-dev-tools": "^3.1.0",
    "prosemirror-schema-basic": "1.2.1111",
    "react": "^18.2.0",
    "react-chartjs-2": "^2.11.2",
    "react-dom": "^18.2.0",
    "react-perfect-scrollbar": "1.5.8",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.3.1",
    "reactstrap": "^8.10.0",
    "rich-markdown-editor": "^11.21.3",
    "svg-captcha": "^1.4.0",
    "tocbot": "^4.13.4",
    "ws": "^8.2.2"
  },
  "peerDependencies": {
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/ws": "^7.4.7"
  }
}
