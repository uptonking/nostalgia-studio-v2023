{
  "name": "minimongo",
  "version": "6.12.61111",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo:idbsync": "webpack serve --hot --config webpack/webpack.config.idbsync.cjs",
    "demo:crdt-hlc": "serve ./crdt-example-app-hlc-jlongster",
    "demo:gdrive": "cd public && serve -C -l 8999",
    "start": "npm run server:nedb",
    "server:nedb": "node --experimental-specifier-resolution=node --loader=ts-node/esm/transpile-only ./tests/draft.test.ts",
    "server": "npm run server:crdt-hlc",
    "test": "npm run test:mini-db",
    "test1": "npm run test:nedb",
    "test:jest": "jest",
    "test:mini-db": "mocha -r jsdom-global/register --file './tests/testing/mocha-jsdom.cjs' --loader=ts-node/esm/transpile-only  --experimental-specifier-resolution=node --extensions ts,tsx 'tests/**/*Tests.ts' --reporter spec --timeout 10000",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "@turf/boolean-crosses": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/boolean-within": "^6.0.1",
    "@turf/intersect": "^6.1.3",
    "async": "^2.0.0",
    "bowser": "^0.7.1",
    "idb-wrapper": "^1.4.1",
    "jquery": "^3.6.0",
    "js-sha1": "^0.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "lolex": "^1.5.0",
    "mocha": "^10.2.0",
    "jsdom": "^20.0.3",
    "jsdom-global": "^3.0.2",
    "mocha-junit-reporter": "^2.2.0",
    "mocha-loader": "^1.1.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1"
  }
}
