{
  "name": "linvodb",
  "version": "3.25.1111",
  "description": "powerful js database designed for airtable-like pivot views, with mongo-like queries",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo:linvo-web": "webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo:gdrive": "cd public && serve -C -l 8999",
    "start": "npm run linvoSync:client",
    "start1": "npm run linvoSync:server",
    "linvoSync:client": "node --experimental-specifier-resolution=node --loader=ts-node/esm/transpile-only ./labs/sync-client.ts",
    "linvoSync:server": "node --experimental-specifier-resolution=node --loader=ts-node/esm/transpile-only ./labs/sync-server.ts",
    "server": "npm run server:crdt-hlc",
    "test": "npm run test:linvo",
    "test1": "npm run test:linvo-oneoff",
    "test:jest": "jest",
    "test:linvo": "mocha --experimental-loader=ts-node/esm/transpile-only --experimental-specifier-resolution=node --extensions ts,js 'tests/**/*.test.ts' --reporter spec --timeout 10000",
    "test:linvo-draft": "node --experimental-specifier-resolution=node --loader=ts-node/esm/transpile-only ./tests/draft.test.ts",
    "test:linvo-oneoff": "mocha --experimental-loader=ts-node/esm/transpile-only --experimental-specifier-resolution=node --extensions ts,js 'tests/text-search.test.ts' --reporter spec --timeout 10000",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "abstract-level": "1.0.3111",
    "async": "^2.0.0",
    "bson": "^1.0.4",
    "hat": "0.0.3",
    "jieba-wasm": "^0.0.2",
    "level-read-stream": "1.1.1111",
    "level-transcoder": "1.0.1111",
    "level-web-stream": "1.1.1111",
    "lodash": "^4.17.21",
    "ws": "^8.13.0",
    "memory-level": "1.0.1111",
    "search-index": "3.4.1111"
  },
  "devDependencies": {
    "@types/async": "^2.0.0",
    "@types/bson": "^1.0.0",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "browser-level": "1.0.1111",
    "chai": "^5.0.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pedding": "^1.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  }
}
