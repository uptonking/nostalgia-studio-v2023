{
  "name": "dnd-kit",
  "version": "0.0.1111",
  "description": "dnd kit – a lightweight React library for building performant and accessible drag and drop experiences",
  "author": "Claudéric Demers",
  "license": "MIT",
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "build:storybook": "build-storybook",
    "prepublish": "lerna run prepublish",
    "start:playground": "yarn run build && yarn --cwd playground && yarn --cwd playground start",
    "start1": "start-storybook -p 6006 -s ./.storybook/assets",
    "start:storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:next": "changeset version --snapshot next",
    "release:next": "changeset publish --tag next"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "3.0.1111",
    "@dnd-kit/core": "6.0.8111",
    "@dnd-kit/modifiers": "6.0.1111",
    "@dnd-kit/sortable": "7.0.2111",
    "@dnd-kit/utilities": "3.2.1111"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "classnames": "^2.5.1",
    "framer-motion": "^7.6.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tiny-virtual-list": "2.2.1111",
    "tsdx": "^0.14.1"
  }
}
